{
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "@babel/plugin-syntax-decorators",
            {
                "decoratorsBeforeExport": true
            }
        ],
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": false,
                "decoratorsBeforeExport": true
            }
        ],
        "@babel/plugin-transform-runtime",
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            },
        ]
    ]
}